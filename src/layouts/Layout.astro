---
// Layout component
export interface Props {
  title: string;
}

const { title }: Props = Astro.props;
---

<html lang="fr">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{title}</title>
  </head>
  <body>
    <h1>Test Layout</h1>
    <slot />
  </body>
</html>

export default Layout;